<body id="salesdashboard">
    <header class="dashboard-header">
        <div class="supervisor-info">
            <div class="avatar"></div>
            <div class="name">Grant Bills</div>
        </div>
        <div class="dashboard-title">
            <div class="date" id="currentDate"></div>
            <h1>Account Repository</h1>
        </div>
    </header>
    <section class="dashboard-navigation">
        <ul>

            <li>
                <a href="#Index">
                    <div id="index" class="navigation-item clear-fix">
                        <div class="icon dashboard"></div>
                        <div>
                            <h2><span> Dashboard</span></h2>
                            <span>Forex Snapshot</span>
                        </div>
                    </div>
                </a>
            </li>
            <li class="phone-hide">
                <a href="#DaysSales">
                    <div id="dayssales" class="navigation-item clear-fix">
                        <div class="icon dailysales"></div>
                        <div>
                            <h2>Forex Manager</h2>
                            <span>Purchases,Sales etc</span>
                        </div>
                    </div>
                </a>
            </li>
            <li>
                <a href="#Products">
                    <div id="products" class="navigation-item clear-fix">
                        <div class="icon products"></div>
                        <div>
                            <h2>Announcement</h2>
                            <span>Send Notifications</span>
                        </div>
                    </div>
                </a>
            </li>
            <li class="phone-hide">
                <a href="#Sectors">
                    <div id="sectors" class="navigation-item clear-fix">
                        <div class="icon sectors"></div>
                        <div>
                            <h2>File Manager</h2>
                            <span>Manage Documents</span>
                        </div>
                    </div>
                </a>
            </li>
            <li>
                <a href="#Regions">
                    <div id="regions" class="navigation-item clear-fix">
                        <div class="icon regions"></div>
                        <div>
                            <h2>Settings</h2>
                            <span>Configurations</span>
                        </div>
                    </div>
                </a>
            </li>
            <li>
                <a href="#Channels">
                    <div id="channels" class="navigation-item clear-fix">
                        <div class="icon channels"></div>
                        <div>
                            <h2>Reporting</h2>
                            <span>Sales, Purchases etc</span>
                        </div>
                    </div>
                </a>
            </li>
            <li class="phone-hide">
                <a href="#Geography">
                    <div id="geography" class="navigation-item clear-fix">
                        <div class="icon geography"></div>
                        <div>
                            <h2>User Manager</h2>
                            <span>Login, registrations</span>
                        </div>
                    </div>
                </a>
            </li>
